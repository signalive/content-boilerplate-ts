{
  "name": "content-boilerplate-modernized",
  "version": "1.0.0",
  "description": "Modernized OmmaWorks content boilerplate with Webpack",
  "main": "src/index.ts",
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production && node build-inline.js",
